pragma solidity ^0.8.20;



contract StakeTracker is ERC20Snapshot {
    using SafeMath for uint256;

    address private immutable _splitter;
    mapping(uint => uint) public rewardQuantity;

    modifier onlySplitter {
        require(msg.sender == _splitter, "Only callable by Splitter");
        _;
    }

    constructor() ERC20("StakeTracker", "ST") {
        _splitter = msg.sender;
    }

    function add(address user, uint quantity) external onlySplitter {
        _mint(user, quantity);
    }

    function remove(address user, uint quantity) external onlySplitter {
        _burn(user, quantity);
    }

    function track(uint quantity) external onlySplitter {
        rewardQuantity[_snapshot()] = quantity;
    }

    function calc(address user, uint[] memory snapshotIds) external view onlySplitter returns (uint) {
        uint quantity = 0;
        for (uint i = 0; i < snapshotIds.length; i++) {
            quantity += getUserReward(user, snapshotIds[i]);
        }
        return quantity;
    }

    function transfer(address, uint256) public pure override returns (bool) {
        revert("Transfers disabled");
    }

    function transferFrom(address, address, uint256) public pure override returns (bool) {
        revert("Transfers disabled");
    }

    function getUserReward(address user, uint snapshotId) public view returns (uint) {
        uint balanceAt = balanceOfAt(user, snapshotId);
        uint supplyAt = totalSupplyAt(snapshotId);
        if (supplyAt == 0) {
            supplyAt = 1; // Avoid dividing by 0
        }
        uint quantity = rewardQuantity[snapshotId]
            .mul(balanceAt)
            .div(supplyAt);
        
        return quantity;
    }

    function getReward(uint snapshotId) external view returns (uint) {
        return rewardQuantity[snapshotId];
    }

    function getCurrentSnapshotId() external view returns (uint) {
        return _getCurrentSnapshotId();
    }

    function getSplitter() external view returns (address) {
        return _splitter;
    }
}

